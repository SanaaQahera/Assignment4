    #include <iostream>
    #include <cmath>
    using namespace std;
     
    int cube(int n)
    {
       int cube = pow(n,3);
       return cube;
    }
     
    int main() {
     
    	int n,c;
    	cout <<"Enter number to be cubed\n";
    	cin >>n;
    	c = cube(n);
    	cout <<"The cube is = "<<c;
     
    	return 0;
    }
